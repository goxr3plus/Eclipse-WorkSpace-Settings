SELECT_ALL_PERSONS = SELECT * FROM person;
SELECT_BY_ID = SELECT * FROM person WHERE id = :id;
DELETE_BY_ID = DELETE FROM person WHERE id = :id;
INSERT_PERSON = INSERT INTO person(id,name,location,birth_date) \
                VALUES (:id,:name,:location,:birth_date);
UPDATE_PERSON = UPDATE person SET name = :name , location = :location , birth_date= :birthday \
                WHERE id = :id "


