package com.intralot.lotos10.messaging.model;

public class MessageDeliveryTypeDTO {

    private Long id;
    private String descr;
    private String name;
    
    
    
    public MessageDeliveryTypeDTO() {
	super();
    }



    public MessageDeliveryTypeDTO(Long id, String descr, String name) {
	super();
	this.id = id;
	this.descr = descr;
	this.name = name;
    }



    public Long getId() {
        return id;
    }



    public void setId(Long id) {
        this.id = id;
    }



    public String getDescr() {
        return descr;
    }



    public void setDescr(String descr) {
        this.descr = descr;
    }



    public String getName() {
        return name;
    }



    public void setName(String name) {
        this.name = name;
    }

    
    
    
}
